<mujoco>
	<compiler angle="radian" coordinate="global" inertiafromgeom="auto"/>
	<option timestep="0.002">
		<flag warmstart="enable"/>
	</option>
	<size nconmax="3000" njmax="3000"/>

	<include file="shared.xml"/>
	<worldbody>
        <light directional="true" ambient="0.2 0.2 0.2" diffuse="0.8 0.8 0.8" specular="0.3 0.3 0.3" castshadow="false" pos="0 0 5" dir="0 0 -1" name="light0"/>
        <!--<light cutoff="45" diffuse=".8 .8 .8" dir="0 0.25 -1" directional="false" name="light" pos="0 -2.0 8.0" specular="0.3 0.3 0.3"/>-->
		<camera quat="0.866 0.500 0.0 0.0" pos="0 -1.8 1.4" name="camera" fovy="25" />

		<include file="cube.xml"/>
		<include file="cuboid.xml"/>
        <body name="table" pos="0 0 -0.01">
			<geom name="table_base" pos="0 0 -0.01" material="table_mat" rgba="0.94 0.70 0.49 1" size="0.5 0.5 0.01" type="box"/>
		</body>
		<body name="table1" pos="11 11 -0.01">
			<geom name="table_base1" pos="11 11 -0.01" material="table_mat" rgba="0.94 0.70 0.49 1" size="2.5 2.5 0.01" type="box"/>
		</body>
	</worldbody>

</mujoco>